{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Effect": "Allow",
			"Action": [
				"kms:ListKeys",
				"kms:ListAliases",
				"kms:CreateKey",
				"kms:CreateAlias",
				"lambda:CreateFunction",
				"lambda:GetEventSourceMapping",
				"lambda:CreateEventSourceMapping"
			],
			"Resource": "*"
		},
		{
			"Effect": "Allow",
			"Action": [
				"kms:GenerateDataKey",
				"kms:Decrypt",
				"kms:DescribeKey",
				"kms:UpdateKeyDescription",
				"kms:GetKeyPolicy",
				"kms:PutKeyPolicy",
				"kms:DeleteAlias",
				"kms:UpdateAlias",
				"kms:ListResourceTags",
				"kms:TagResource",
				"kms:UntagResource",
				"kms:ListResourceTags",
				"kms:ScheduleKeyDeletion",
				"kms:CancelKeyDeletion",
				"kms:CreateGrant",
				"kms:EnableKeyRotation",
				"kms:GetKeyRotationStatus"
			],
			"Resource": [
				"arn:aws:kms:*:${ACCOUNT_ID}:alias/*",
				"arn:aws:kms:*:${ACCOUNT_ID}:key/*"
			]
		},
		{
			"Effect": "Allow",
			"Action": [
				"lambda:GetFunction",
				"lambda:TagResource",
				"lambda:GetFunctionConfiguration",
				"lambda:DeleteFunction",
				"lambda:GetFunctionCodeSigningConfig",
				"lambda:PutFunctionConcurrency",
				"lambda:ListVersionsByFunction",
				"lambda:ListTags",
				"lambda:UntagResource",
				"lambda:AddPermission",
				"lambda:GetPolicy",
				"lambda:RemovePermission"
			],
			"Resource": [
				"arn:aws:lambda:*:${ACCOUNT_ID}:function:llandman-queue-processing"
			]
		},
		{
			"Effect": "Allow",
			"Action": [
				"lambda:DeleteEventSourceMapping"
			],
			"Resource": "arn:aws:lambda:*:${ACCOUNT_ID}:event-source-mapping:*"
		},
		{
			"Effect": "Allow",
			"Action": [
				"dynamodb:DescribeTimeToLive",
				"dynamodb:DeleteTable",
				"dynamodb:DescribeTable",
				"dynamodb:CreateTable",
				"dynamodb:ListTagsOfResource",
				"dynamodb:UpdateContinuousBackups",
				"dynamodb:TagResource",
				"dynamodb:UntagResource",
				"dynamodb:DescribeContinuousBackups",
				"dynamodb:UpdateTable"
			],
			"Resource": "arn:aws:dynamodb:*:${ACCOUNT_ID}:table/llandman-model-outputs"
		},
		{
			"Effect": "Allow",
			"Action": [
				"iam:ListRoleTags",
				"iam:PutRolePolicy",
				"iam:ListAttachedRolePolicies",
				"iam:CreateRole",
				"iam:ListRolePolicies",
				"iam:GetRole",
				"iam:DeleteRole",
				"iam:UntagRole",
				"iam:TagRole",
				"iam:GetRolePolicy",
				"iam:ListInstanceProfilesForRole",
				"iam:PassRole",
				"iam:DeleteRolePolicy",
				"iam:GetPolicy",
				"iam:GetPolicyVersion",
				"iam:AttachRolePolicy",
				"iam:DetachRolePolicy",
				"iam:ListPolicies"
			],
			"Resource": [
				"arn:aws:iam::${ACCOUNT_ID}:role/*",
				"arn:aws:iam::${ACCOUNT_ID}:*",
				"arn:aws:iam::aws:policy/service-role/*"
			]
		},
		{
			"Effect": "Allow",
			"Action": [
				"s3:GetObject",
                "s3:PutObject",
                "s3:PutObjectAcl",
                "s3:GetObjectTagging",
                "s3:GetBucketObjectLockConfiguration",
                "s3:DeleteObject",
                "s3:DeleteObjectVersion",
                "s3:PutObjectVersionAcl",
                "s3:ListBucket",
                "s3:GetBucketAcl",
                "s3:GetBucketCORS",
                "s3:GetBucketNotification",
                "s3:GetBucketPolicy",
                "s3:GetBucketRequestPayment",
                "s3:GetBucketWebsite",
                "s3:GetBucketLocation",
                "s3:GetBucketObjectLockConfiguration",
                "s3:GetBucketPolicyStatus",
                "s3:GetBucketTagging",
                "s3:GetBucketLogging",
                "s3:GetBucketOwnershipControls",
                "s3:PutBucketNotification"
			],
			"Resource": [
				"arn:aws:s3:::enverus-tfstates",
				"arn:aws:s3:::enverus-tfstates/*",
				"arn:aws:s3:::enverus-courthouse-dev-chd-plants-0816",
				"arn:aws:s3:::enverus-courthouse-dev-chd-plants-0816/*"
			]
		},
		{
			"Effect": "Allow",
			"Action": [
				"sqs:UntagQueue",
				"sqs:CreateQueue",
				"sqs:TagQueue",
				"sqs:GetQueueAttributes",
				"sqs:DeleteQueue",
				"sqs:SetQueueAttributes",
				"sqs:ListQueueTags"
			],
			"Resource": [
				"arn:aws:sqs:*:${ACCOUNT_ID}:llandman-queue",
				"arn:aws:sqs:*:${ACCOUNT_ID}:llandman-dlq"
			]
		},
		{
			"Effect": "Allow",
			"Action": [
				"bedrock:GetPrompt",
				"bedrock:ListTagsForResource",
				"bedrock:CreatePrompt",
				"bedrock:UpdatePrompt",
				"bedrock:ListPrompts",
				"bedrock:GetFoundationModelAvailability",
				"bedrock:ListFoundationModels",
				"bedrock:DeletePrompt"
			],
			"Resource": [
				"arn:aws:bedrock:us-east-1:${ACCOUNT_ID}:prompt/*",
				"arn:aws:bedrock:*::foundation-model/*"
			]
		},
		{
			"Effect": "Allow",
			"Action": [
				"cloudformation:CreateResource",
				"cloudformation:GetResource",
				"cloudformation:DeleteResource",
				"cloudformation:GetResourceRequestStatus"
			],
			"Resource": "arn:aws:cloudformation:us-east-1:${ACCOUNT_ID}:resource/*"
		}
	]
}