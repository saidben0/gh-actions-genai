{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "VisualEditor11",
            "Effect": "Allow",
            "Action": [
                "kms:*",
                "lambda:CreateFunction",
                "kms:ListAliases",
                "kms:CreateKey",
                "kms:CreateAlias",
                "lambda:GetEventSourceMapping",
                "lambda:CreateEventSourceMapping"
            ],
            "Resource": "*"
        },
        {
            "Sid": "VisualEditor1",
            "Effect": "Allow",
            "Action": [
                "kms:DescribeKey",
                "kms:GetKeyPolicy",
                "kms:ListResourceTags",
                "kms:TagResource",
                "kms:DeleteAlias",
                "kms:UntagResource",
                "kms:ListResourceTags",
                "kms:ScheduleKeyDeletion",
                "kms:CreateGrant",
                "kms:EnableKeyRotation",
                "kms:GetKeyRotationStatus"
            ],
            "Resource": [
                "arn:aws:kms:*:${ACCOUNT_ID}:alias/*",
                "arn:aws:kms:*:${ACCOUNT_ID}:key/*"
            ]
        },
        {
            "Sid": "VisualEditor2",
            "Effect": "Allow",
            "Action": [
                "lambda:GetFunction",
                "lambda:CreateFunction",
                "lambda:TagResource",
                "lambda:GetFunctionConfiguration",
                "lambda:DeleteFunction",
                "lambda:GetFunctionCodeSigningConfig",
                "lambda:PutFunctionConcurrency",
                "lambda:ListVersionsByFunction",
                "lambda:ListTags",
                "lambda:UntagResource"
            ],
            "Resource": [
                "arn:aws:lambda:*:${ACCOUNT_ID}:function:*"
            ]
        },
        {
            "Sid": "VisualEditor33",
            "Effect": "Allow",
            "Action": [
                "lambda:DeleteEventSourceMapping"
            ],
            "Resource": "arn:aws:lambda:*:${ACCOUNT_ID}:event-source-mapping:*"
        },
        {
            "Sid": "VisualEditor3",
            "Effect": "Allow",
            "Action": [
                "dynamodb:DescribeTimeToLive",
                "dynamodb:DeleteTable",
                "dynamodb:DescribeTable",
                "dynamodb:CreateTable",
                "dynamodb:ListTagsOfResource",
                "dynamodb:UpdateContinuousBackups",
                "dynamodb:TagResource",
                "dynamodb:UntagResource",
                "dynamodb:DescribeContinuousBackups"
            ],
            "Resource": "arn:aws:dynamodb:*:${ACCOUNT_ID}:table/*"
        },
        {
            "Sid": "VisualEditor4",
            "Effect": "Allow",
            "Action": [
                "iam:ListRoleTags",
                "iam:PutRolePolicy",
                "iam:ListAttachedRolePolicies",
                "iam:CreateRole",
                "iam:ListRolePolicies",
                "iam:GetRole",
                "iam:DeleteRole",
                "iam:UntagRole",
                "iam:TagRole",
                "iam:GetRolePolicy",
                "iam:ListInstanceProfilesForRole",
                "iam:PassRole",
                "iam:DeleteRolePolicy"
            ],
            "Resource": "arn:aws:iam::${ACCOUNT_ID}:role/*"
        },
        {
            "Effect": "Allow",
            "Action": [
                "s3:GetObject",
                "s3:PutObject"
            ],
            "Resource": [
                "arn:aws:s3:::*",
                "arn:aws:s3:::*/*"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "s3:CreateBucket",
                "s3:DeleteBucket",
                "s3:ListBucket",
                "s3:GetBucketLocation",
                "s3:PutBucketPolicy",
                "s3:DeleteBucketPolicy",
                "s3:PutBucketAcl",
                "s3:PutBucketCors",
                "s3:PutBucketLogging",
                "s3:PutBucketNotification",
                "s3:PutBucketTagging",
                "s3:PutBucketWebsite",
                "s3:DeleteBucketWebsite",
                "s3:ListTagsForResource",
                "s3:GetBucketTagging",
                "s3:TagResource",
                "s3:UntagResource"
            ],
            "Resource": "arn:aws:s3:::*"
        },
        {
            "Sid": "VisualEditor6",
            "Effect": "Allow",
            "Action": [
                "sqs:UntagQueue",
                "sqs:CreateQueue",
                "sqs:TagQueue",
                "sqs:GetQueueAttributes",
                "sqs:DeleteQueue",
                "sqs:SetQueueAttributes",
                "sqs:ListQueueTags"
            ],
            "Resource": "arn:aws:sqs:*:${ACCOUNT_ID}:*"
        }
    ]
}
